# Database
DB_PASSWORD=stockcerebro_dev_password

# Google OAuth2 Configuration
# Get your credentials from: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# JWT Secret Key (generate a strong random key for production)
# You can generate one with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=dev-secret-change-in-production

# Cookie Security Settings (set to true/strict in production with HTTPS)
COOKIE_SECURE=false
COOKIE_SAMESITE=lax

# Finnhub (free tier - get key at https://finnhub.io/)
FINNHUB_API_KEY=your_finnhub_api_key_here

# SEC EDGAR (required: your email as user agent)
EDGAR_USER_AGENT=your_email@example.com
